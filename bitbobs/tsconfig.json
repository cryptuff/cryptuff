  "compilerOptions": {
    "target": "es2016",
    "skipLibCheck": false,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve",
    "allowJs": true
  },
  "include": [
    "src"
  ]
}
